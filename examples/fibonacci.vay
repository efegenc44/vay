module Main

import Numbers (Nat (Nat (Succ, Zero)))
import Core (Addable, Bool)

fun main() : Bool =
    let three = Succ(Succ(Succ(Zero))) in
    let seven = Succ(three + three) in
    let eigth = Succ(seven) in
    let twentyone = three * seven in
        fibonacci(eigth) == twentyone

fun fibonacci(n : Nat) : Nat = match n {
    let .Zero           : Zero
    let .Succ(.Zero)    : Succ(Zero)
    let .Succ(.Succ(n)) : fibonacci(Succ(n)) + fibonacci(n)
}
